<div class="articleHomeHeader">
  <h1>Articles</h1>
  <a href="/articles/new">Add a New Article</a>
</div>
<br>
<div class = 'article-entries-container'>
  {{#each articles}}
  <div id='articles-entries'>
  <div>
    TITLE:
    <a href="/articles/{{this.title}}">{{this.title}}</a>
  </div>
  <h5>Author: {{this.author}}</h5>
  <p id = 'article-body'>BODY: {{this.body}}</p>
  <p>Url Title: {{this.urlTitle}}</p>
  </div>
  {{/each}}
</div>