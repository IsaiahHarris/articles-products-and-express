<div class="articleHomeWrapper">
<div class="articleHomeHeader">
  <form action="/articles/new">
  <input class = 'anew' type="submit" value='Add a New Article'>
  </form>
  <h1 id = 'article-h1'>Articles</h1>
  <form action="/">
  <input type="submit" value="HOME" class = 'home-button'>
  </form>
</div>
<div class = 'article-entries-container'>
  {{#each articles}}
  <div id='articles-entries'>  
    <form action="/articles/{{this.id}}">
    <input type="submit" value="{{this.title}}" class = 'atitle'>
    </form> 
  <h5>Author: {{this.author}}</h5>
  <p class = 'article-body'>BODY: {{this.body}}</p>
  <p>Url Title: {{this.urltitle}}</p>
  </div>
  {{/each}}
</div>